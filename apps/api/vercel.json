{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/main.prod.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.prod.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run build && npx prisma generate"
}
